{
	"name": "@ctx-core/socicon",
	"version": "8.2.57",
	"description": "ctx-core socicon",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/socicon#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/socicon/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/socicon.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./bin",
		"./[icon_name].svg",
		"./ui"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"refresh-socicon": "./bin/refresh-socicon.mjs"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/array": "^27.1.13",
		"@ctx-core/cli-args": "^11.4.52",
		"@ctx-core/object": "^31.1.33",
		"@ctx-core/svg-ui-svelte": "^1.3.58",
		"htmlparser2": "^9.0.0",
		"svelte": "^4.2.8"
	},
	"devDependencies": {
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.6.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}