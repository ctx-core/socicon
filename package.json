{
	"name": "@ctx-core/socicon",
	"version": "5.0.3",
	"description": "ctx-core socicon",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/socicon#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/socicon/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/socicon/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile && npm run symlink",
		"clean": "rm -f tsconfig.tsbuildinfo",
		"compile": "./node_modules/.bin/tsc -b .",
		"symlink": "./node_modules/.bin/symlink_src_assets_to_dist .",
		"exec": "$@",
		"prepublishOnly": "npm run compile"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^21.0.2",
		"@ctx-core/combinators": "workspace:^6.3.1",
		"@ctx-core/function": "workspace:^18.0.1",
		"@ctx-core/html": "workspace:^9.0.0",
		"@ctx-core/object": "workspace:^19.0.1",
		"@ctx-core/svg": "workspace:^11.0.3",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/cli-args": "workspace:^8.0.2",
		"@ctx-core/ctx-core-package-tools": "workspace:^7.0.1",
		"esm": "^3.2.25",
		"globby": "^11.0.3",
		"htmlparser2": "^6.1.0",
		"resolve": "^1.20.0",
		"socicon": "^3.0.5",
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
