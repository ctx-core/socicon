{
	"name": "@ctx-core/socicon",
	"version": "7.0.79",
	"description": "ctx-core socicon",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/socicon#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/socicon/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/socicon/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"bin": {
		"refresh-socicon": "./bin/refresh-socicon.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.23",
		"@ctx-core/combinators": "workspace:^8.0.17",
		"@ctx-core/function": "workspace:^20.2.3",
		"@ctx-core/html": "workspace:^11.0.19",
		"@ctx-core/object": "workspace:^21.0.22",
		"@ctx-core/svg": "workspace:^13.0.79",
		"svelte": "3.44.1"
	},
	"devDependencies": {
		"@ctx-core/cli-args": "workspace:^11.0.0",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.77",
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.107",
		"globby": "^12.0.2",
		"htmlparser2": "^7.1.2",
		"resolve": "^1.20.0",
		"socicon": "^3.0.5",
		"typescript": "^4.4.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
