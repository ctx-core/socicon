{
	"name": "@ctx-core/socicon",
	"version": "7.0.8",
	"description": "ctx-core socicon",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/socicon#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/socicon/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/socicon/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"bin": {
		"refresh-socicon": "./bin/refresh-socicon.mjs"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run copy-components && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"require = require('esm')(module/*, options*/)\nmodule.exports = require('./main.js')\nexport * from './index.js'\" > index.cjs",
		"copy-components": "cd src && find . | grep \\.svelte$ | awk '{print \"mkdir -p dirname \"$1\" && cp -f \"$1\" ../dist/\"$1}'",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.6",
		"@ctx-core/combinators": "workspace:^8.0.6",
		"@ctx-core/function": "workspace:^20.0.5",
		"@ctx-core/html": "workspace:^11.0.6",
		"@ctx-core/object": "workspace:^21.0.5",
		"@ctx-core/svg": "workspace:^13.0.7",
		"svelte": "3.38.3"
	},
	"devDependencies": {
		"@ctx-core/cli-args": "workspace:^10.0.6",
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.29",
		"esm": "^3.2.25",
		"globby": "^11.0.4",
		"htmlparser2": "^6.1.0",
		"resolve": "^1.20.0",
		"socicon": "^3.0.5",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
