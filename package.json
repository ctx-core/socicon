{
	"name": "@ctx-core/socicon",
	"version": "8.0.27",
	"description": "ctx-core socicon",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/socicon#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/socicon/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/socicon.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"svelte": "./lib/index.js",
	"exports": {
		".": "./lib/index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"refresh-socicon": "./bin/refresh-socicon.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^26.2.0",
		"@ctx-core/cli-args": "workspace:^11.0.31",
		"@ctx-core/object": "workspace:^22.1.7",
		"@ctx-core/svg-ui-svelte": "workspace:^1.1.27",
		"htmlparser2": "^8.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.171",
		"rimraf": "^3.0.2",
		"svelte": "^3.47.0",
		"svelte-preprocess": "^4.10.6",
		"svelte2tsx": "^0.5.9",
		"tslib": "^2.4.0",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}