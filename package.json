{
	"name": "@ctx-core/socicon",
	"version": "8.0.85",
	"description": "ctx-core socicon",
	"keywords": [
		"ctx-core",
		"fontawesome"
	],
	"homepage": "https://github.com/ctx-core/socicon#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/socicon/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/socicon.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./lib/index.d.ts",
	"svelte": "./lib/index.js",
	"exports": {
		".": "./lib/index.js",
		"./package.json": "./package.json"
	},
	"bin": {
		"refresh-socicon": "./bin/refresh-socicon.mjs"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf lib && npm run clean:tsbuildinfo",
		"clean:tsbuildinfo": "rimraf tsconfig.tsbuildinfo && rimraf lib/**/*.d.ts",
		"compile": "npm run compile:source && npm run compile:declaration",
		"compile:source": "swc src --out-dir lib --copy-files --source-maps --config-file .swcrc",
		"compile:declaration": "npm run clean:tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir lib",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "^26.8.15",
		"@ctx-core/cli-args": "^11.0.64",
		"@ctx-core/object": "^24.1.2",
		"@ctx-core/svg-ui-svelte": "^1.2.33",
		"htmlparser2": "^8.0.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@swc/cli": "^0.1.60",
		"@swc/core": "^1.3.32",
		"@types/resolve": "^1.20.2",
		"rimraf": "^4.1.2",
		"svelte": "^3.55.1",
		"svelte-preprocess": "^5.0.1",
		"svelte2tsx": "^0.6.0",
		"tslib": "^2.5.0",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}